@charset "utf-8";
/*
 * reset base
 * author gaomy
 * date   2015-06-25
*/
a, address, article, aside, audio, b, big, blockquote, body, canvas, caption, center, cite, code, dd, div, dl, dt, em, embed, fieldset, figcaption, figure, font, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, hr, html, i, iframe, img,  label, legend, li, mark, menu, nav, object, ol, output, p, pre, progress, q, s, samp, section, small, span, strike, strong, sub, summary, sup, table, tbody, td, tfoot, th, thead, time, tr, tt, u, ul, var, video {
	margin: 0;
	padding: 0;
	font-size: 100%;
}
html, body {
	height: 100%;
	background-attachment: fixed;
	font-size: 12px;
	line-height: 22px;
	font-family: "SimSun", Arial, Tahoma, Geneva, sans-serif;
}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display: block; }
fieldset { border: none; }
i, em, span, b {
	font-style: normal;
	display: inline-block;
}
img {
	vertical-align: middle;
	border: none;
	-ms-interpolation-mode: bicubic;
}
li, ul {
	display: list-item;
	list-style: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
th, td, caption { font-weight: normal; }
a {
	text-decoration: none;
	outline: none;
}
input, label, button, select { vertical-align: middle; }
input,button {
	margin: 0;
	padding: 0;
	box-sizing: content-box;
	-webkit-box-sizing: content-box;
	-moz-box-sizing: content-box;
}
input,textarea{
	outline: 0;
	-webkit-tap-highlight-color: rgba(255,0,0,0);
}
button {
	border: none;
	cursor: pointer;
}
.fl {
	float: left;
	display: inline;
}
.fr {
	float: right;
	display: inline;
}
.undis, .h { display: none; }
.dis { display: block; }
.clear { clear: both; }
.clearfix:after {
	clear: both;
	display: block;
	height: 0;
	content: "";
	visibility: hidden;
}
